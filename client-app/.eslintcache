y of cropper in PhotoWidgetCropper\n\tfunction onCrop() {\n\t\tif (cropper) {\n\t\t\tcropper.getCroppedCanvas().toBlob(blob => uploadPhoto(blob!));\n\t\t}\n\t}\n\n\t// clean up 'preview: URL.createObjectURL(file)' (from PhotoImageDropzone) from memory after use\n\tuseEffect(() => {\n\t\treturn () => {\n\t\t\tfiles.forEach((file: any) => URL.revokeObjectURL(file.preview));\n\t\t}\n\t}, [files]);\n\n\treturn (\n\t\t<Grid>\n\t\t\t<Grid.Column width={4}>\n\t\t\t\t<Header sub color='teal' content='Add an image'/>\n\t\t\t\t<PhotoWidgetDropzone setFiles={setFiles}/>\n\t\t\t</Grid.Column>\n\t\t\t<Grid.Column width={1}/>\n\t\t\t<Grid.Column width={4}>\n\t\t\t\t<Header sub color='teal' content='Adjust your image'/>\n\t\t\t\t{files && files.length > 0 && (\n\t\t\t\t\t<PhotoWidgetCropper setCropper={setCropper} imagePreview={files[0].preview}/>\n\t\t\t\t)}\n\t\t\t</Grid.Column>\n\t\t\t<Grid.Column width={1}/>\n\t\t\t<Grid.Column width={4}>\n\t\t\t\t<Header sub color='teal' content='Preview and upload your image'/>\n\t\t\t\t{files && files.length > 0 &&\n\t\t\t\t\t<>\n\t\t\t\t\t\t<div className='img-preview' style={{minHeight: 200, overflow: 'hidden'}}/>\n\t\t\t\t\t\t<Button.Group widths={2} style={{paddingTop: 10}}>\n\t\t\t\t\t\t\t<Button onClick={onCrop} positive icon='check' loading={loading}/>\n\t\t\t\t\t\t\t<Button onClick={() => setFiles([])} color='red' icon='close' disabled={loading}/>\n\t\t\t\t\t\t</Button.Group>\n\n\t\t\t\t\t</>\n\t\t\t\t}\n\t\t\t</Grid.Column>\n\t\t</Grid>\n\t);\n}\n\n\n// {files && files.length > 0 && (\n// \t<Image src={files[0].preview} />\n// )}","/Users/matt/Documents/ASPnet/eventspace/client-app/src/app/common/imageUpload/PhotoWidgetDropzone.tsx",[],"/Users/matt/Documents/ASPnet/eventspace/client-app/src/app/common/imageUpload/PhotoWidgetCropper.tsx",[],{"ruleId":"196","replacedBy":"197"},{"ruleId":"198","replacedBy":"199"},{"ruleId":"200","severity":1,"message":"201","line":1,"column":30,"nodeType":"202","messageId":"203","endLine":1,"endColumn